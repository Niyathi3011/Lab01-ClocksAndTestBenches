#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55c5c1c8a940 .scope module, "lab01_tb" "lab01_tb" 2 18;
 .timescale -9 -12;
v0x55c5c1ca3220_0 .var "clk", 0 0;
v0x55c5c1ca32c0_0 .var/i "count", 31 0;
v0x55c5c1ca3380_0 .var/i "failedTests", 31 0;
v0x55c5c1ca3440_0 .var/i "high_count", 31 0;
v0x55c5c1ca3520_0 .var "last_tick", 0 0;
v0x55c5c1ca3630_0 .var "reset", 0 0;
v0x55c5c1ca36f0_0 .net "tick_100_2", 0 0, L_0x55c5c1c72a40;  1 drivers
v0x55c5c1ca3790_0 .var/i "totalTests", 31 0;
v0x55c5c1ca3850_0 .var/i "transition_count", 31 0;
E_0x55c5c1c8b2d0 .event negedge, v0x55c5c1ca3630_0;
S_0x55c5c1c8aad0 .scope module, "uut_100_2" "gen_tick" 2 43, 3 16 0, S_0x55c5c1c8a940;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "src_clk";
    .port_info 1 /INPUT 1 "enable";
    .port_info 2 /OUTPUT 1 "tick";
P_0x55c5c1c6fc60 .param/l "SRC_FREQ" 0 3 16, +C4<00000000000000000000000001100100>;
P_0x55c5c1c6fca0 .param/l "TICK_FREQ" 0 3 16, +C4<00000000000000000000000000000010>;
L_0x55c5c1c72a40 .functor BUFZ 1, v0x55c5c1ca30e0_0, C4<0>, C4<0>, C4<0>;
v0x55c5c1c72b60_0 .var "acc", 31 0;
L_0x7f5bf42cd018 .functor BUFT 1, C4<1>, C4<0>, C4<0>, C4<0>;
v0x55c5c1c731f0_0 .net "enable", 0 0, L_0x7f5bf42cd018;  1 drivers
v0x55c5c1ca2e50_0 .var "limit", 31 0;
v0x55c5c1ca2f10_0 .net "src_clk", 0 0, v0x55c5c1ca3220_0;  1 drivers
v0x55c5c1ca2fd0_0 .net "tick", 0 0, L_0x55c5c1c72a40;  alias, 1 drivers
v0x55c5c1ca30e0_0 .var "tick_out", 0 0;
E_0x55c5c1c8b7b0 .event posedge, v0x55c5c1ca2f10_0;
    .scope S_0x55c5c1c8aad0;
T_0 ;
    %pushi/vec4 24, 0, 32;
    %store/vec4 v0x55c5c1ca2e50_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55c5c1c72b60_0, 0, 32;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55c5c1ca30e0_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x55c5c1c8aad0;
T_1 ;
    %wait E_0x55c5c1c8b7b0;
    %load/vec4 v0x55c5c1c731f0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %load/vec4 v0x55c5c1c72b60_0;
    %addi 1, 0, 32;
    %assign/vec4 v0x55c5c1c72b60_0, 0;
    %load/vec4 v0x55c5c1c72b60_0;
    %load/vec4 v0x55c5c1ca2e50_0;
    %cmp/e;
    %jmp/0xz  T_1.2, 4;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x55c5c1c72b60_0, 0;
    %load/vec4 v0x55c5c1ca30e0_0;
    %inv;
    %assign/vec4 v0x55c5c1ca30e0_0, 0;
T_1.2 ;
T_1.0 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55c5c1c8a940;
T_2 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55c5c1ca3790_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55c5c1ca3380_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55c5c1ca32c0_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55c5c1ca3440_0, 0, 32;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55c5c1ca3520_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55c5c1ca3850_0, 0, 32;
    %end;
    .thread T_2;
    .scope S_0x55c5c1c8a940;
T_3 ;
    %vpi_call 2 31 "$dumpfile", "lab01.vcd" {0 0 0};
    %vpi_call 2 32 "$dumpvars", 32'sb00000000000000000000000000000000 {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x55c5c1c8a940;
T_4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55c5c1ca3220_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55c5c1ca3630_0, 0, 1;
    %delay 50000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55c5c1ca3220_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55c5c1ca3630_0, 0, 1;
    %delay 50000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55c5c1ca3220_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55c5c1ca3630_0, 0, 1;
    %delay 50000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55c5c1ca3220_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55c5c1ca3630_0, 0, 1;
    %delay 50000, 0;
T_4.0 ;
    %load/vec4 v0x55c5c1ca3220_0;
    %inv;
    %store/vec4 v0x55c5c1ca3220_0, 0, 1;
    %delay 50000, 0;
    %jmp T_4.0;
    %end;
    .thread T_4;
    .scope S_0x55c5c1c8a940;
T_5 ;
    %wait E_0x55c5c1c8b2d0;
    %wait E_0x55c5c1c8b7b0;
    %delay 10000, 0;
    %vpi_call 2 83 "$write", "Test Source clock 100Hz, Tick 2Hz ... " {0 0 0};
    %pushi/vec4 1, 0, 32;
    %assign/vec4 v0x55c5c1ca3790_0, 0;
T_5.0 ;
    %load/vec4 v0x55c5c1ca32c0_0;
    %cmpi/s 1000, 0, 32;
    %jmp/0xz T_5.1, 5;
    %wait E_0x55c5c1c8b7b0;
    %load/vec4 v0x55c5c1ca3520_0;
    %pad/u 32;
    %pushi/vec4 0, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x55c5c1ca36f0_0;
    %load/vec4 v0x55c5c1ca3520_0;
    %cmp/ne;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_5.2, 8;
    %load/vec4 v0x55c5c1ca3850_0;
    %addi 1, 0, 32;
    %assign/vec4 v0x55c5c1ca3850_0, 0;
T_5.2 ;
    %load/vec4 v0x55c5c1ca32c0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x55c5c1ca32c0_0, 0, 32;
    %load/vec4 v0x55c5c1ca36f0_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_5.4, 4;
    %load/vec4 v0x55c5c1ca3440_0;
    %addi 1, 0, 32;
    %assign/vec4 v0x55c5c1ca3440_0, 0;
T_5.4 ;
    %load/vec4 v0x55c5c1ca36f0_0;
    %assign/vec4 v0x55c5c1ca3520_0, 0;
    %jmp T_5.0;
T_5.1 ;
    %load/vec4 v0x55c5c1ca3440_0;
    %pushi/vec4 500, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x55c5c1ca3850_0;
    %pushi/vec4 20, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_5.6, 8;
    %vpi_call 2 98 "$display", "PASSED" {0 0 0};
    %jmp T_5.7;
T_5.6 ;
    %vpi_call 2 100 "$display", "FAILED" {0 0 0};
    %load/vec4 v0x55c5c1ca3380_0;
    %addi 1, 0, 32;
    %store/vec4 v0x55c5c1ca3380_0, 0, 32;
T_5.7 ;
    %pushi/real 1073741824, 4066; load=1.00000
    %load/vec4 v0x55c5c1ca3440_0;
    %cvt/rv/s;
    %mul/wr;
    %load/vec4 v0x55c5c1ca32c0_0;
    %cvt/rv/s;
    %div/wr;
    %vpi_call 2 103 "$display", "Load (%d/%d): %0.2f", v0x55c5c1ca3440_0, v0x55c5c1ca32c0_0, W<0,r> {0 1 0};
    %vpi_call 2 104 "$display", "Transition count: %d", v0x55c5c1ca3850_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55c5c1ca3520_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55c5c1ca3850_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55c5c1ca32c0_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55c5c1ca3440_0, 0, 32;
    %vpi_call 2 117 "$finish" {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
